{
  "name": "Picture To Tumblr",
  "version": "1.2.6",
  "description": "Quickly way to post picture to tumblr",
	"permissions": [ "contextMenus","tabs","http://www.tumblr.com/api/write"],
  "background_page": "background.html",
	"options_page": "options.html",
	"icons": {
		"48": "icon_48.png",
		"128": "icon_64.png"
	},
	"content_scripts": [
	  {
	    "matches": ["http://*/*","https://*/*"],
			"js": ["jquery-1.4.2.min.js", "contentscript.js"]
		}
	]
}